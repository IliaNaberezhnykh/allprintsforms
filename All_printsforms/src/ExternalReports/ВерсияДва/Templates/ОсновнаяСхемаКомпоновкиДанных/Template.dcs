	ВТ_Измен.КоличествоПосле КАК КоличествоПосле
ИЗ
	ВТ_Основной КАК ВТ_Основной
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Измен КАК ВТ_Измен
		ПО ВТ_Основной.Идентификатор = ВТ_Измен.Идентификатор
			И ВТ_Основной.Событие = ВТ_Измен.Событие
			И ВТ_Основной.ДатаИзменения = ВТ_Измен.ДатаИзменения
